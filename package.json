{
  "name": "next-koa2-typescript-starter",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev_api": "concurrently \"tsc --watch\" \"nodemon dest/api/index.js\"",
    "server_next": "next ./dest",
